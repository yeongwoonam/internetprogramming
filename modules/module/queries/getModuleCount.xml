<query id="getModuleCount" action="select">
    <tables>
        <table name="modules" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="site_srl" var="site_srl" />
        <condition operation="equal" column="module" var="module" pipe="and" />
    </conditions>
</query>
